{
  "name": "cocoon-ynab",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "dist/components.js",
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@cocoon/plugin-distance": "0.87.0",
    "@cocoon/plugin-echarts": "0.87.0",
    "@cocoon/plugin-merge": "0.87.0",
    "@cocoon/rollup": "0.87.0",
    "@cocoon/types": "0.87.0",
    "@zeit/ncc": "0.20.2",
    "prettier": "1.18.2"
  },
  "scripts": {
    "build": "yarn build:ncc && yarn build:rollup",
    "build:ncc": "ncc build -m -e lodash index.ts",
    "build:rollup": "rollup --config rollup.config.js",
    "dev:ncc": "ncc build -mw -e lodash index.ts",
    "dev:rollup": "DEBUG=1 rollup --config rollup.config.js --watch"
  },
  "dependencies": {
    "@types/form-data": "2.2.1",
    "@types/got": "9.6.0",
    "form-data": "2.4.0",
    "got": "9.6.0",
    "ynab": "1.13.4"
  }
}
