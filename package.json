{
  "name": "cocoon-ynab",
  "version": "0.1.0",
  "main": "dist/index.js",
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "cocoon": {
    "nodes": [
      "dist/CreateTransactions",
      "dist/ReadN26",
      "dist/ReadYNAB"
    ],
    "views": [
      {
        "module": "dist/TransactionModule",
        "component": "dist/TransactionComponent"
      }
    ]
  },
  "devDependencies": {
    "@cocoon/plugin-distance": "0.108.0",
    "@cocoon/plugin-echarts": "0.108.0",
    "@cocoon/rollup": "0.108.0",
    "@cocoon/types": "0.108.0",
    "@zeit/ncc": "0.20.4",
    "prettier": "1.18.2"
  },
  "scripts": {
    "postinstall": "yarn build",
    "build": "rollup --config rollup.config.js",
    "dev": "DEBUG=1 rollup --config rollup.config.js --watch",
    "editor": "cocoon-editor cocoon.yml"
  },
  "dependencies": {
    "@cocoon/editor": "0.108.0",
    "@cocoon/util": "0.108.0",
    "@types/form-data": "2.5.0",
    "@types/got": "9.6.7",
    "form-data": "2.5.1",
    "got": "9.6.0",
    "ynab": "1.16.0"
  }
}
